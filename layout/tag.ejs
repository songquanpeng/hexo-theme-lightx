<article id="tag">
  <h1><%=page.tag%></h1>
  <% page.posts.each(article => { %>
    <div class="tag-item">
      <a href="<%=url_for(article.path)%>">
        <%=article.title%>
      </a>
      <time datetime="<%=date_xml(article.date)%>">
        <%=date(article.date)%>
      </time>
    </div>
  <% }) %>
</article>
<%- partial('partials/paginator') %>

